{
  "compilerOptions": {
    "module": "commonjs",
    "noEmitOnError": false,
    "noImplicitAny": true,
    "removeComments": true,
    "suppressImplicitAnyIndexErrors": true,
    "target": "es5"
  },
  "compileOnSave": false,
  "buildOnSave": false,
  "filesGlob": [
    "src/**/*.ts",
    "!src/**/*.spec.ts",
    "typings/**/*.d.ts"
  ],
  "atom": {
    "rewriteTsconfig": true
  },
  "files": [
    "src/Data.ts",
    "src/index.ts",
    "src/Monomer.ts",
    "src/scroll.ts",
    "src/Scroller.ts",
    "src/Sequence.ts",
    "src/sequences/1l2y.ts",
    "src/sequences/4ins.ts",
    "src/state.ts",
    "typings/globals/eventemitter3/index.d.ts",
    "typings/globals/node/index.d.ts",
    "typings/index.d.ts"
  ]
}
